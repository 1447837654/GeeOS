import lib.io
import trap.trap
import mem.mem
import proc.thread

// supervisor mode entry of GeeOS
public def main() {
  io <<< "initializing trap handler...\n"
  initTrap()
  io <<< "initializing memory...\n"
  initMem()
  io <<< "initializing threads...\n"
  initThread()
  io <<< "done!\n\n"
  runCpu()
}
